{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.4",
    "acoustep/entrust-gui": "5.2.x-dev",
    "barryvdh/laravel-ide-helper": "^2.3",
    "baum/baum": "^1.0",
    "laravel/framework": "5.3.31",
    "caffeinated/themes": "^3.0",
    "cviebrock/eloquent-sluggable": "^3.1",
    "doctrine/dbal": "^2.5",
    "filp/whoops": "^2.1",
    "guzzlehttp/guzzle": "^6.2",
    "laravelcollective/html": "^5.2",
    "spatie/laravel-backup": "^4.0",
    "pusher/pusher-php-server": "^2.3",
    "rtconner/laravel-tagging": "^2.2",
    "thomaswelton/laravel-gravatar": "^1.0",
    "vespakoen/menu": "^3.0",
    "yajra/laravel-datatables-oracle": "^6.0",
    "nwidart/laravel-menus": "~0.2",
    "nwidart/laravel-modules": "^1.13",
    "floatingpoint/stylist": "~0.1",
    "maatwebsite/laravel-sidebar": "~2.1",
    "zizaco/entrust": "5.2.x-dev"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.5",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-2.0": "2.0.x-dev"
    },
    "merge-plugin": {
      "include": [
        "Modules/*/composer.json"
      ]
    }
  }
}
